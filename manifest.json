{
    "manifest_version": 3,
    "name": "warp",
    "description": "Super fast jump to bookmarks",
    "version": "0.1.0",
    "permissions": [
        "storage",
        "activeTab",
        "bookmarks"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "commands": {
        "run-foo": {
            "suggested_key": {
                "windows": "Ctrl+Shift+4",
                "mac": "Command+Shift+4",
                "chromeos": "Ctrl+Shift+4",
                "linux": "Ctrl+Shift+4"
            },
            "description": "Run \"foo\" on the current page.",
            "global": false
        },
        "_execute_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+5",
                "mac": "Command+Shift+5",
                "chromeos": "Ctrl+Shift+5",
                "linux": "Ctrl+Shift+5"
            }
        }
    }
}