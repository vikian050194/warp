{
    "manifest_version": 3,
    "name": "warp",
    "description": "Open bookmarks as fast as possible without clicking",
    "version": "0.1.0",
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "bookmarks"
    ],
    "background": {
        "service_worker": "background/index.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "commands": {
        "foo-command": {
            "suggested_key": {
                "windows": "Ctrl+Shift+1",
                "mac": "Command+Shift+1",
                "chromeos": "Ctrl+Shift+1",
                "linux": "Ctrl+Shift+1"
            },
            "description": "Send foo-command to the background.js",
            "global": false
        },
        "bar-command": {
            "suggested_key": {
                "windows": "Ctrl+Shift+2",
                "mac": "Command+Shift+2",
                "chromeos": "Ctrl+Shift+2",
                "linux": "Ctrl+Shift+2"
            },
            "description": "Send bar-command to the background.js",
            "global": false
        },
        "_execute_action": {
            "suggested_key": {
                "windows": "Alt+W",
                "mac": "Command+W",
                "chromeos": "Alt+W",
                "linux": "Alt+W"
            }
        }
    }
}