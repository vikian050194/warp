{
    "manifest_version": 3,
    "name": "warp",
    "description": "Open bookmarks as fast as possible without clicking",
    "version": "0.1.0",
    "permissions": [
        "storage",
        "bookmarks"
    ],
    "background": {
        "service_worker": "background/index.js",
        "type": "module"
    },
    "action": {
        "default_icon": "icons/icon_32.png",
        "default_popup": "popup/popup.html"
    },
    "options_page": "options.html",
    "options_ui": {
        "page": "options/options.html",
        "open_in_tab": true
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "windows": "Alt+W",
                "mac": "Command+W",
                "chromeos": "Alt+W",
                "linux": "Alt+W"
            }
        }
    },
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "128": "icons/icon_128.png"
    }
}